{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    
    "rules": {
        "curly": "warn",
        "callback-return": "warn",
        "array-bracket-spacing": "warn",
        "semi":"warn",
        "no-console":"off",
        "indent": ["error", 2],
        "no-unused-vars":"warn"
    }
}